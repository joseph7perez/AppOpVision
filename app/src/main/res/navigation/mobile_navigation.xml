        android:name="com.example.myapplication.InicioFragment"
        android:label="@string/menu_inicio"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/nav_tomarFoto"
        android:name="com.example.myapplication.TomarFotoFragment"
        android:label="@string/menu_tomar_foto"
