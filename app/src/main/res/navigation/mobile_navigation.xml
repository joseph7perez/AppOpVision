    <fragment
        android:id="@+id/nav_subirFoto"
        android:name="com.example.myapplication.ui.SubirFoto.SubirFotoFragment"
        android:label="@string/menu_subir_foto"
        tools:layout="@layout/fragment_subir_foto" />

    <fragment
        android:id="@+id/nav_configuracion"
